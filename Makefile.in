VPATH = @srcdir@
srcdir = @srcdir@
top_srcdir = @top_srcdir@
prefix = @prefix@
datarootdir = @datarootdir@
datadir = @datadir@
infodir = @infodir@
M5_VERSION = @M5_VERSION@

all: .version

.version: Makefile
	$(srcdir)/build-aux/git-version-gen .tarball-version > .version

dist-clean: clean
	rm -rf autom4te.cache config.log config.status configure

clean:
	rm -rf Makefile .version
